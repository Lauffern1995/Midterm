<!DOCTYPE html>
<html lang="en">

<head>
  <title>MapWiki, the free location encyclopedia</title>


  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
  <link rel="stylesheet" href="/styles/navbar.css" type="text/css" />

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Lato:wght@300&display=swap" rel="stylesheet">

  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>
  <script type="text/javascript" src="/routes/index.js"></script>


</head>

<%- include('partials/header') %>

<body>
  <div class="map-container">
    <div class="info">
      <%- include('partials/new_map') %>
      <div class="maps-form">
      </div>
      <div class="user-maps-list">
        <form method="post" action="/getusermaps">
          <button type="submit">List Your Maps!</button>
        </form>

        <!-- remove this if in the way -->
        <%- include('partials/test') %>

        <% if (locals.maps) { %>
          <ul id="map-list">
            <% maps.map(function(map, i){ %>
            <li>
                <% console.log('inside ejs', map.title) %>
                 <%- `<a href=''>${map.title}</a>` %>
                </li>
                <% }); %>
              </ul>
          <% } else { %>
          <ul id="map-list">
          </ul>
          <% } %>
      </div>
    </div>
    <div id="map"></div>
  </div>

  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDtIjXHDQ9Blne5H3PfOpa1xohVP5gbpS4"
  async defer></script>
</body>
</html>
